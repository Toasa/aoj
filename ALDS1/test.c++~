#include <iostream>
#include <iomanip>
#include <cstdlib>
#include <cstdio>
#include <string>
#include <vector>
#include <set>
#include <list>
#include <map>
#include <algorithm>
#include <typeinfo>
#define _USE_MATH_DEFINES
#include <cmath>
#include <stack>
#include <queue>

using namespace std;

void twice(vector<int> &a) {
  for (int i = 0; i < 3; i++) {
	a[i] *= a[i];
  }
}

int main() {

  vector<int> v(3);
  v[0] = 3;
  v[1] = 5;
  v[2] = 7;

  cout << v[0] << endl;
  twice(v);
  cout << v[0] << endl;

}
